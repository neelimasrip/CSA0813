a=int(input("enter the value of a"))
b=int(input("enter the value of b"))
c=int(input("enter the value of c"))
if(a>b):
    if(a>c):
        print("a is greater number")
    else:
        print("c is greater number")
else:
        if(b>=c):
           print("b is greater number")
        else:
           print("c is greater number")
        
